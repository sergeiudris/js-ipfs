{
  "name": "ipfs-message-port-client",
  "version": "0.4.1",
  "description": "IPFS client library for accessing IPFS node over message port",
  "keywords": [
    "ipfs",
    "message-port",
    "worker"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "src/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "browser": {},
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "aegir build",
    "build:types": "tsc --build",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "browser-readablestream-to-it": "^1.0.1",
    "ipfs-message-port-protocol": "^0.4.1"
  },
  "devDependencies": {
    "aegir": "^28.2.0",
    "interface-ipfs-core": "^0.142.1",
    "ipfs": "^0.52.1",
    "ipfs-core": "^0.2.1",
    "ipfs-message-port-server": "^0.4.1",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=3.0.0"
  }
}
