{
  "name": "ipfs-http-client",
  "version": "48.1.1",
  "description": "A client library for the IPFS HTTP API",
  "keywords": [
    "ipfs"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "src/index.js",
  "browser": {
    "./src/lib/multipart-request.js": "./src/lib/multipart-request.browser.js",
    "ipfs-utils/src/files/glob-source": false,
    "go-ipfs": false,
    "ipfs-core-utils/src/files/normalise-input": "ipfs-core-utils/src/files/normalise-input/index.browser.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:js": "aegir build",
    "build:types": "tsc --build",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "any-signal": "^2.0.0",
    "bignumber.js": "^9.0.0",
    "cids": "^1.0.0",
    "debug": "^4.1.1",
    "form-data": "^3.0.0",
    "ipfs-core-utils": "^0.5.2",
    "ipfs-utils": "^5.0.0",
    "ipld-block": "^0.11.0",
    "ipld-dag-cbor": "^0.17.0",
    "ipld-dag-pb": "^0.20.0",
    "ipld-raw": "^6.0.0",
    "it-last": "^1.0.4",
    "it-map": "^1.0.4",
    "it-tar": "^1.2.2",
    "it-to-stream": "^0.1.2",
    "merge-options": "^2.0.0",
    "multiaddr": "^8.0.0",
    "multibase": "^3.0.0",
    "multicodec": "^2.0.1",
    "multihashes": "^3.0.1",
    "nanoid": "^3.1.12",
    "native-abort-controller": "~0.0.3",
    "parse-duration": "^0.4.4",
    "stream-to-it": "^0.2.2",
    "uint8arrays": "^1.1.0"
  },
  "devDependencies": {
    "aegir": "^28.2.0",
    "go-ipfs": "^0.7.0",
    "ipfs-core": "^0.2.1",
    "ipfsd-ctl": "^7.0.2",
    "it-all": "^1.0.4",
    "it-concat": "^1.0.1",
    "nock": "^13.0.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=3.0.0"
  }
}
