{
  "name": "ipfs-message-port-server",
  "version": "0.4.1",
  "description": "IPFS server library for exposing IPFS node over message port",
  "keywords": [
    "ipfs",
    "message-port",
    "worker"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "src/index.js",
  "browser": {},
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "aegir build",
    "build:types": "tsc --build",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "ipfs-message-port-protocol": "^0.4.1",
    "it-all": "^1.0.4"
  },
  "devDependencies": {
    "@types/it-all": "^1.0.0",
    "aegir": "^28.2.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.3.0",
    "npm": ">=3.0.0"
  }
}
