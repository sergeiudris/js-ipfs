{
  "name": "ipfs",
  "version": "0.52.1",
  "description": "JavaScript implementation of the IPFS specification",
  "keywords": [
    "IPFS"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "src/index.js",
  "bin": {
    "jsipfs": "src/cli.js"
  },
  "scripts": {
    "build": "npm run build:bundle && npm run build:types",
    "build:bundle": "aegir build",
    "build:types": "tsc --build",
    "clean": "rm -rf ./dist"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "ipfs-cli": "^0.2.1",
    "ipfs-core": "^0.2.1",
    "ipfs-repo": "^7.0.0",
    "semver": "^7.3.2",
    "update-notifier": "^5.0.0"
  },
  "devDependencies": {
    "aegir": "^28.2.0",
    "cross-env": "^7.0.0",
    "electron-webrtc": "^0.3.0",
    "go-ipfs": "^0.7.0",
    "interface-ipfs-core": "^0.142.1",
    "ipfs-http-client": "^48.1.1",
    "ipfs-interop": "^3.0.0",
    "ipfs-utils": "^5.0.0",
    "ipfsd-ctl": "^7.0.2",
    "iso-url": "^1.0.0",
    "libp2p-webrtc-star": "^0.20.1",
    "merge-options": "^2.0.0",
    "typescript": "^4.0.3",
    "wrtc": "^0.4.6"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*",
        "dist/*/index"
      ]
    }
  },
  "browser": {
    "./src/cli.js": false,
    "go-ipfs": false
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
